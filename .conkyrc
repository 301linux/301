background yes
use_xft yes
xftfont Sans:size=8
xftalpha 1
update_interval 5.0
total_run_times 0
own_window yes
own_window_transparent yes
own_window_type desktop
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
minimum_size 200 200
maximum_width 200
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders yes
default_color white
default_shade_color black
default_outline_color white
alignment top_right
gap_x 0
gap_y 0
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale no

TEXT
${font sans-serif:bold:size=8}301_linux ${hr 2}
${font sans-serif:normal:size=7}$sysname $kernel $alignr $machine
Host:$alignr$nodename
Uptime:$alignr$uptime
${color red}${font sans-serif:bold:size=12}EST:$alignr${time %H:%M:%S}
${color white}${font sans-serif:normal:size=7}Day:$alignr${time %A %m.%d.%y}
${font sans-serif:bold:size=7}PROCESSORS ${hr 2}
${font sans-serif:normal:size=7}CPU(S): $alignr ${cpu cpu1}% | ${cpu cpu2}% 
Frequency: $alignr  $freq MHz 
Temperature: $alignr  ${hwmon 0 temp 1} C 
${font sans-serif:bold:size=7}MEMORY ${hr 2}
${font sans-serif:normal:size=7}I/O: $alignr ${diskio_read sda} | ${diskio_write sda}
${font sans-serif:normal:size=7}RAM  $alignr $mem / $memmax 
${font sans-serif:normal:size=7}/ $alignr ${fs_used /} / ${fs_size /} 
${font sans-serif:normal:size=7}SWAP $alignr ${swap} / ${swapmax} 
${font sans-serif:bold:size=7}TOP PROCESSES ${hr 2}
${font sans-serif:normal:size=7}${top_mem name 1}${alignr}${top mem 1} %
${font sans-serif:normal:size=7}${top_mem name 2}${alignr}${top mem 2} %
${font sans-serif:normal:size=7}${top_mem name 3}${alignr}${top mem 3} %
