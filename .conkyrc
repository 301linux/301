${font sans-serif:normal:size=7}${top_mem name 5}${alignr}${top mem 5} %
${font sans-serif:normal:size=7}${top_mem name 6}${alignr}${top mem 6} %
${font sans-serif:bold:size=8}NETWORK${hr 2}${font sans-serif:normal:size=7}
Node:$alignr${addr eth0}
IP:$alignr${execi 3600 wget -O - http://ip.tupeux.com | tail}
In:$alignr${downspeed eth0}
Out:$alignr${upspeed eth0s0}
